 <%= simple_form_for car_wash do |form| %>
    <%= form.input :name, label: 'Name' %>
    <%= form.input :description, label: 'Description' %>
    <%= form.input :price_sedan, label: 'Price sedan' %>
    <%= form.input :price_suv, label: 'Price suv' %>
    <%= form.input :price_truck, label: 'Price truck' %>
    <%= form.input :price_luxury, label: 'Price luxury' %>
    <%= form.input :location, label: 'Location' %>
    <%= form.button :submit, 'Create car wash', class: 'btn btn-primary w-100' %>
  <% end %>
