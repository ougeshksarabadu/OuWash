<div class="container border mt-3 p-2">
  <h1 class="mt-4">Car Wash</h1>
  <%= simple_form_for @car_wash do |form| %>
    <%= form.input :name, label: 'Name' %>
     <%= form.input :price_sedan, label: 'Price_sedan' %>
      <%= form.input :price_suv, label: 'Price_suv' %>
       <%= form.input :price_truck, label: 'Price_truck' %>
        <%= form.input :price_luxury, label: 'Price_luxury' %>
    <%= form.button :submit, 'Create car wash', class: 'btn btn-primary w-100' %>
  <% end %>
</div>


